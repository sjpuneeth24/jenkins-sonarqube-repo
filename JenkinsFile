pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compile..'
                bat gradle clean
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('SonarQube') {
            steps {
                echo 'SonarQube....'
                bat gradle sonarqube
            }
        }
    }
}
